                        <div class="input-container">
                            <img loading="lazy"
                                src="https://cdn.builder.io/api/v1/image/assets/TEMP/617a38b568aaf31062f7197e0aae5d496eeb9be6d676c9e8a019f5c453b9981b?apiKey=d20e9c94b7d84fc1a58e2a177cbb4ca0&"
                                class="input-icon" />
                            <input type="text" class="input-field" asp-for="Identity"
                                placeholder="Identity Number (NPWP/NIB)" required />
                        </div>
                        @* <div class="div-10">&gt; Search results based on (NPWP/NIB)</div> *@
                        <div class="input-container">
                            <img loading="lazy"
                                src="https://cdn.builder.io/api/v1/image/assets/TEMP/b3aeff3678ebe87c5c601541c7392af44f4cd2a17d7e676713009eb4a6dda9cb?apiKey=d20e9c94b7d84fc1a58e2a177cbb4ca0&"
