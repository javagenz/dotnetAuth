                        <div class="input-container">
                            <img loading="lazy"
                                src="https://cdn.builder.io/api/v1/image/assets/TEMP/e257f2a283d4750b38e92c2533c1a66eb75a9fa66b428654e36ba5dd1ea90f2c?apiKey=d20e9c94b7d84fc1a58e2a177cbb4ca0&"
                                class="input-icon" />
