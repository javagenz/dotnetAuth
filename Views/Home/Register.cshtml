                        <div class="input-container">
                            <img loading="lazy"
                                src="https://cdn.builder.io/api/v1/image/assets/TEMP/507ac1e824ff0b556980881a81a589680b933b65f573f52ad15ba9ed404bc933?apiKey=d20e9c94b7d84fc1a58e2a177cbb4ca0&"
